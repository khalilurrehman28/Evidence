<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <View
            android:layout_width="wrap_content"
            android:layout_height="2px"
            android:visibility="visible"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="2dp"
            android:background="#dfdfdf" />
        <LinearLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="6dp"
            android:orientation="horizontal"
            android:layout_toRightOf="@+id/thumbnail"
            android:layout_centerVertical="true">

            <TextView
                android:id="@+id/frameworkSubTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Framework Title"
                android:textColor="#3e3e3e"
                android:typeface="sans"
                android:layout_marginRight="8dp"
                android:layout_weight="1"
                android:textSize="18dp"
                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="4dp">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll1"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="1"
                    android:background="#e42f2f"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll2"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="2"
                    android:background="#ef4e4e"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll3"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="3"
                    android:background="#f76262"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll4"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="4"
                    android:background="#f5e082"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll5"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="5"
                    android:background="#FDD835"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll6"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#efcd37"
                    android:text="6"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll7"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#8fd993"
                    android:text="7"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll8"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#66BB6A"
                    android:text="8"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll9"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#43A047"
                    android:text="9"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:id="@+id/ll10"
                android:background="#212121"
                android:layout_height="45dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="10"
                    android:background="#2E7D32"
                    android:gravity="center"
                    android:textSize="19dp"
                    android:textColor="@color/white"/>

            </LinearLayout>

        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:layout_marginLeft="2dp"
            android:orientation="horizontal"
            android:layout_toRightOf="@+id/thumbnail"
            android:layout_centerVertical="true">

            <TextView
                android:id="@+id/frameworkDes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Framework Des"
                android:textColor="@color/grey"
                android:typeface="sans"
                android:layout_marginRight="8dp"
                android:layout_weight="1"
                android:textSize="16dp"
                />


        </LinearLayout>
    </LinearLayout>



</LinearLayout>